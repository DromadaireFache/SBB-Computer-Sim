global_y = 0
global_x = 1
global_n = 13

start:
    lda 	global_n
    cmp# 	0
    jpne 	&local1
    halt
    *local1
    *loop_local2
    lda 	global_x
    add 	global_y
    sta 	global_y
    take
    sta 	global_x
    lda 	global_n
    dec
    sta 	global_n
    cmp# 	0
    jpne 	&loop_local2
    lda 	global_y
    halt