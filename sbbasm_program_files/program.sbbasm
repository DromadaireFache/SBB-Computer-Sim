global_y = 0
global_x = 1
global_n = 13

global_main:
    lda 	global_n
    cmp# 	0
    jpne 	&local1
    ret
    *local1
    *loop_local2
    lda 	global_y
    add 	global_x
    sta 	global_y
    sub 	global_x
    sta 	global_x
    lda 	global_n
    dec
    sta 	global_n
    cmp# 	0
    jpne 	&loop_local2
    lda 	global_y
    ret

start:
    jsr 	global_main
    halt